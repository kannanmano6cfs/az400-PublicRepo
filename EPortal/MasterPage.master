               <div id="col1" 
                    style="border-width: 0px; background-color: #669999; text-align: center;">      
                <asp:Image ID="Image1" runat="server" Height="171px" 
            ImageUrl="~/new header.jpg" Width="1178px" />        
             </div>
            </td>
        </tr>
        <tr>
            <td class="style2" colspan="3" align="center"               
                style="border-width: 0px; text-align: center;">
                
                </td>
    </tr>
    <tr>
    <td colspan="3" class="style5">
    <div id="col2">     
        <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
        </asp:ContentPlaceHolder>
    </div>
    </td>
    </tr>
    <tr>
